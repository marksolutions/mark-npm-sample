  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
          "name": "feature/*",
          "prerelease": "${name.replace(/^feature\\//g, \"\")}",
          "channel": "${name.replace(/^feature\\//g, \"\")}"
      }
    ]
  }
}
